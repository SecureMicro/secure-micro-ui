        "axios": "^1.7.7",
        "formik": "^2.4.6",
        "lucide-react": "^0.454.0",
        "moment": "^2.30.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.27.0",
        "react-toastify": "^10.0.6",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@types/axios": "^0.9.36",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.3",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.15.0",
